#include <stdio.h>

int main() {
    int i,sum=0;
    int newno[5]={};
    int no[5]={};
    for(i=0;i<=4;i++)
    {
        printf("enter %d mark",i);
        scanf("%d",&no[i]);
    }
    for(i=0;i<=4;i++)
    {
        printf("%d",no[i]);
    }
    printf("\n");
    
    
    for(i=0;i<=4;i++)
    {
        newno[i]=no[5-i-1];
        printf("%d",newno[i]);
    }

  
}
